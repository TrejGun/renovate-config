{
  "extends": [
    "github>ethberry/renovate-config:rules"
  ],
  "hostRules": [
    {
      "matchHost": "https://npm.pkg.github.com/",
      "hostType": "npm",
      "token": "{{ secrets.GITHUB_TOKEN_TREJGUN }}"
    }
  ],
  "npmrc": "@ethberry:registry=https://npm.pkg.github.com/",
  "packageRules": [
    {
      "updateTypes": ["minor", "patch"],
      "automerge": true
    }
  ]
}
